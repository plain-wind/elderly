{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ESNext",
    "baseUrl": ".",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "@amap/amap-jsapi-types",
      "auto-imports.d.ts",
      "components.d.ts",
    ]
  },
}